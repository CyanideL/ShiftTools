    <mobi.cyann.shifttools.preference.RemovablePreferenceCategory android:title="@string/label_idle_mode" >
    <mobi.cyann.shifttools.preference.IntegerPreference
        custom:interfacePath="/sys/module/cpuidle_exynos4/parameters/enable_mask"
        android:key="@string/key_idle_mode"
            custom:maxValue="3"
            custom:minValue="0"
            custom:step="1"
	    custom:description="   0 = IDLE,   1 = AFTR\n2 = IDLE+LPA,   3 = AFTR+LPA (Deep Sleep)"
            android:summary="0"
        android:title="@string/title_idle_mode" />
   </mobi.cyann.shifttools.preference.RemovablePreferenceCategory>

    <mobi.cyann.shifttools.preference.RemovablePreferenceCategory android:title="@string/label_governor" >
